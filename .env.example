# Example environment variables
# Copy this to .env and adjust values for your environment

# Server Configuration
PORT=3000
BASE_URL=http://localhost:3000

# File Upload Configuration
MAX_FILE_SIZE_BYTES=5368709120  # 5GB in bytes
BODY_SIZE_LIMIT=1gb

# Session and Cleanup Configuration
SESSION_TIMEOUT_MS=900000       # 15 minutes in milliseconds
CLEANUP_INTERVAL_MS=300000      # 5 minutes in milliseconds

# File Retention
FILE_RETENTION_TIME=30          # How long to keep uploaded files in minutes
